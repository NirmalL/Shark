<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="394.0" prefWidth="648.0">
         <children>
            <TableView fx:id="table_packets" layoutX="7.0" layoutY="6.0" prefHeight="381.0" prefWidth="388.0">
              <columns>
                <!--<TableColumn fx:id="col_time" prefWidth="42.0" />-->
                <TableColumn fx:id="col_source" prefWidth="92.0" text="Source" />
                  <TableColumn fx:id="col_destination" prefWidth="91.0" text="Destination" />
                  <TableColumn fx:id="col_size" prefWidth="75.0" text="Size" />
                  <TableColumn fx:id="col_type" prefWidth="75.0" text="Type" />
              </columns>
            </TableView>
            <Accordion layoutX="402.0" layoutY="6.0" prefHeight="381.0" prefWidth="239.0">
              <panes>
                <TitledPane text="Open file">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TextField fx:id="txt_file" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="209.0" promptText="/path/file.*cap">
                                 <effect>
                                    <Reflection />
                                 </effect></TextField>
                              <Button fx:id="btn_open" layoutX="93.0" layoutY="49.0" mnemonicParsing="false" text="Open">
                                 <effect>
                                    <Reflection />
                                 </effect></Button>
                           </children>
                        </AnchorPane>
                  </content>
                </TitledPane>
                <TitledPane text="Filter">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TextArea fx:id="txt_filters" layoutX="7.0" layoutY="30.0" prefHeight="76.0" prefWidth="223.0" promptText="terms">
                                 <effect>
                                    <Reflection />
                                 </effect></TextArea>
                              <Button fx:id="btn_filter" layoutX="52.0" layoutY="117.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Apply">
                                 <effect>
                                    <Reflection />
                                 </effect></Button>
                              <CheckBox fx:id="chk_filter" layoutX="7.0" layoutY="6.0" mnemonicParsing="false" selected="true" text="Filter" />
                              <Button fx:id="btn_filter_clear" layoutX="124.0" layoutY="117.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="61.0" text="Clear">
                                 <effect>
                                    <Reflection />
                                 </effect>
                              </Button>
                           </children></AnchorPane>
                  </content>
                </TitledPane>
              </panes>
            </Accordion>
         </children>
      </AnchorPane>
   </children>
</GridPane>
